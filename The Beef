Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"covariance", " ", "=", " ", 
   RowBox[{"Covariance", "[", "Iris", " ", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Labeled", "[", 
  RowBox[{
   RowBox[{"covariance", "//", "MatrixForm"}], ",", 
   "\"\<Covariance Matrix: \>\"", ",", "Left"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"eigenvectors", " ", "=", " ", 
   RowBox[{"Transpose", "[", 
    RowBox[{"Eigenvectors", "[", "covariance", "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Labeled", "[", 
  RowBox[{
   RowBox[{"eigenvectors", "//", "MatrixForm"}], ",", 
   "\"\<Eigen Vectors: \\n(As Columns)\>\"", ",", "Left"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"eigenvalues", " ", "=", " ", 
   RowBox[{"Eigenvalues", "[", "covariance", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Labeled", "[", 
  RowBox[{
   RowBox[{"eigenvalues", "//", "MatrixForm"}], ",", "\"\<Eigen Values: \>\"",
    ",", "Left"}], "]"}]}], "Input",
 CellChangeTimes->{{3.8166861772303915`*^9, 3.816686198279957*^9}, 
   3.8166925993677816`*^9, {3.81669301657783*^9, 3.8166930169552655`*^9}, {
   3.816698095525799*^9, 3.8166981969089613`*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"ee58a231-1f72-4619-9f53-64f414d6acce"],

Cell[BoxData[
 TemplateBox[{
   TagBox[
