Cell[BoxData[{
 RowBox[{
  RowBox[{"fruitMat", "=", 
   RowBox[{"fruits", "[", 
    RowBox[{"[", 
     RowBox[{"All", ",", " ", 
      RowBox[{"1", ";;", "2"}]}], "]"}], "]"}]}], 
